{
  "name": "@libp2p/protocol-adventure",
  "version": "1.0.2",
  "description": "Learn how to develop custom protocols with libp2p",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/libp2p/js-libp2p-protocol-adventure#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/js-libp2p-protocol-adventure.git"
  },
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p-protocol-adventure/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "bin": {
    "protocol-adventure": "./index.js"
  },
  "files": [
    "index.js",
    "exercises",
    "i18n",
    "lib",
    "patches"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "revert": true,
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "deps",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "section": "Trivial Changes"
              },
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "deps",
                "section": "Dependencies"
              },
              {
                "type": "test",
                "section": "Tests"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "lint": "aegir lint",
    "postinstall": "patch-package",
    "test": "aegir test -t node",
    "test:node": "aegir test -t node --cov",
    "release": "aegir release"
  },
  "dependencies": {
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@libp2p/tcp": "^10.0.11",
    "@libp2p/tls": "^2.0.10",
    "cborg": "^4.2.2",
    "it-all": "^3.0.6",
    "it-byte-stream": "^2.0.2",
    "it-filter": "^3.1.1",
    "it-length-prefixed-stream": "^2.0.2",
    "it-map": "^3.1.1",
    "it-pipe": "^3.0.1",
    "it-rpc": "^1.0.1",
    "libp2p": "^2.2.1",
    "patch-package": "^8.0.0",
    "uint8-varint": "^2.0.4",
    "uint8arraylist": "^2.4.8",
    "workshopper-adventure": "^7.0.0",
    "workshopper-exercise": "^3.0.1",
    "workshopper-verify-processor": "0.0.2"
  },
  "devDependencies": {
    "aegir": "^47.0.12"
  }
}
